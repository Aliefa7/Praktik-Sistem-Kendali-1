>> syms u t
>> % JAWABAN SOAL NOMOR 2
>> % Soal 2(a)
>> fta = u*(t+9)
fta =
 
u*(t + 9)
 
>> flapa = laplace(fta)
flapa =
 
(u*(9*s + 1))/s^2
 
>> % Soal 2(b)
>> ftb = exp(5*t)
ftb =
 
exp(5*t)
 
>> flapab = laplace(ftb)
flapb =
 
1/(s - 5)

>> % Soal 2(c)
>> ftc = (5*t + 7)
ftc =
 
5*t + 7
 
>> flalc = laplace(ftc)
flalc =
 
(5*((7*s)/5 + 1))/s^2
